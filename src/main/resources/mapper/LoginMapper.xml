<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.desonz.stom.member.LoginMapper.LoginMapper">

    <!-- 로그인 -->
    <select id="loginCheck" parameterType="map" resultType="com.desonz.stom.member.LoginModel.LoginModel">
        SELECT * FROM user_login
        where user_id = #{user_id}
        and user_pw = #{user_pw}
    </select>


</mapper>